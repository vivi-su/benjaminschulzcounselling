    z-index: 2;   
}

/* style-wraper */

.style-container{
    position:relative;   
}

.style{
    border:1px solid var(--color-primary);
    position: absolute;
    top:20%;
    left:20%;
    z-index:1;
}

.style-wraper{
    width:70%;
    height: 0;
    padding-bottom: 70%;
}

@media screen and (min-width:768px){
  
    .style-wraper{
        width:72%;
        height: 0;
        padding-bottom: 72%;
    }
    
}

@media screen and (min-width:1024px){
  
    .style-wraper{
        width:65%;
        height: 0;
        padding-bottom: 65%;
    }
    
}

/* faq */

.faq--block{
    background-color: var(--color-accent); 
}

.faq__image{
    width:80%;
    margin:0 auto;
    
}

.faq__content{
    color:#fff;
    padding:2rem;
    text-align:left;
}

.faq__heading{
    margin:0;
    padding-bottom: 1.5rem;
    text-align: left;
    text-transform: uppercase;
    color:#fff;
}

.faq__item{
    padding:1rem 0;
}

.faq__item__heading{
    color:#fff;
}

.faq__image{
    width:80%;
