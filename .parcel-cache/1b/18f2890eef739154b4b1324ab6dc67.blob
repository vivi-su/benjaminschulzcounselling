;\n} \n\n*,\n*::after,\n*::before {\n  box-sizing: border-box;\n}\n\na{\n    text-decoration: none;\n    color:var(--color-secondary);\n}\n\n.container{\n    max-width:1440px;\n    margin:0 auto;\n}\n\nbody{\n    font-family: 'Open Sans', Arial, Helvetica, sans-serif;\n    color:#fff;\n    font-size:1.563rem;\n    line-height:1.5;\n    font-weight: 300;\n}\n\nh1,\nh2,\nh3{\n    font-family: 'Roboto', Arial, Helvetica, sans-serif;\n    margin-bottom: 1rem;\n}\n\nh1{\n    color:#fff;\n}\n\nh2,h3,h4{\n    color:var(--color-secondary);\n}\n\nh1{\n    font-size: 3.052rem;\n}\nh2{\n    font-size:3rem;\n    letter-spacing: 0.1rem;\n}\nh3{\n    font-size:2.5rem;\n}\n\n.lead{\n    font-size: 1.563rem;\n}\n\n.lead-second{\n    font-size:1.2rem;\n    white-space: nowrap;\n    text-align: center;\n    margin:2rem auto;\n}\n\n.strong{\n    font-weight: 700;\n}\n\n.dark{\n    color:var(--color-secondary);\n}\n\n.light{\n    color:var(--color-primary);\n}\n\n@media screen and (min-width:600px){\n    body{\n        font-size:2rem;\n    }\n    h1{\n        font-size:6rem;\n    }\n    h2{\n        font-size:3.998rem;\n    }\n    h3{\n        font-size:2.827rem;\n    }\n    .lead{\n        font-size: 2rem;\n    }\n    .lead-second{\n        font-size:1.8rem;\n        letter-spacing: 1px;\n    }\n}\n\n@media screen and (min-width:768px){\n  \n    h1{\n        font-size:7rem;\n    }\n   \n}\n\n@media screen and (min-width:1024px){\n    body{\n        font-size:2rem;\n    }\n    h1{\n        font-size:10rem;\n    }\n    h2{\n        font-size:4.5rem;\n    }\n    h3{\n        font-size:3.375rem;\n    }\n    .lead{\n        font-size: 2.2rem;\n    }\n    .lead-second{\n        font-size:2rem;\n        letter-spacing: 2px;\n    }\n}\n\n/* background image */\n.cover-image{\n   background: url(../images/hero-small.webp) no-repeat;\n   background-size: cover;\n   min-width:100%; \n   min-height:100vh;\n   background-position: center center;\n \n}\n\n.no-webp .cover-image{\n    background: url(../images/hero-small.jpg) no-repeat;\n   background-size: cover; \n}\n\n@media\nonly screen and (-webkit-min-device-pixel-ratio: 2)      and (min-width: 320px),\nonly screen and (   min--moz-device-pixel-ratio: 2)      and (min-width: 320px),\nonly screen and (     -o-min-device-pixel-ratio: 2/1)    and (min-width: 320px),\nonly screen and (        min-device-pixel-ratio: 2)      and (min-width: 320px),\nonly screen and (                min-resolution: 192dpi) and (min-width: 320px),\nonly screen and (                min-resolution: 2dppx)  and (min-width: 320px) { \n\n    .cover-image{\n        background: url(../images/hero-small@2x.webp) no-repeat;\n        background-size: cover;\n     }\n     \n     .no-webp .cover-image{\n         background: url(../images/hero-small@2x.jpg) no-repeat;\n        background-size: cover; \n     }\n\n}\n\n@media screen and (min-width:768px){\n    .cover-image{\n        background: url(../images/hero-medium.webp) no-repeat;\n        background-size: cover;\n    }\n    .no-webp .cover-image{\n        background: url(../images/hero-medium.jpg) no-repeat;\n       background-size: cover; \n    }\n}\n  \n  @media\n  only screen and (-webkit-min-device-pixel-ratio: 2)      and (min-width: 600px),\n  only screen and (   min--moz-device-pixel-ratio: 2)      and (min-width: 600px),\n  only screen and (     -o-min-device-pixel-ratio: 2/1)    and (min-width: 600px),\n  only screen and (        min-device-pixel-ratio: 2)      and (min-width: 600px),\n  only screen and (                min-resolution: 192dpi) and (min-width: 600px),\n  only screen and (                min-resolution: 2dppx)  and (min-width: 600px) { \n    .cover-image{\n        background: url(../images/hero-medium@2x.webp) no-repeat;\n        background-size: cover;\n    }\n    .no-webp .cover-image{\n        background: url(../images/hero-medium@2x.jpg) no-repeat;\n       background-size: cover; \n    }\n\n  \n  }\n\n@media screen and (min-width:1024px){\n    .cover-image{\n        background: url(../images/hero-large.webp) no-repeat;\n        background-size: cover;\n    }\n    .no-webp .cover-image{\n        background: url(../images/hero-large.jpg) no-repeat;\n       background-size: cover; \n    }\n}\n\n@media\nonly screen and (-webkit-min-device-pixel-ratio: 2)      and (min-width: 1300px),\nonly screen and (   min--moz-device-pixel-ratio: 2)      and (min-width: 1300px),\nonly screen and (     -o-min-device-pixel-ratio: 2/1)    and (min-width: 1300px),\nonly screen and (        min-device-pixel-ratio: 2)      and (min-width: 1300px),\nonly screen and (                min-resolution: 192dpi) and (min-width: 1300px),\nonly screen and (                min-resolution: 2dppx)  and (min-width: 1300px) { \n    .cover-image{\n        background: url(../images/hero-large@2x.webp) no-repeat;\n        background-size: cover;\n    }\n    .no-webp .cover-image{\n        background: url(../images/hero-large@2x.jpg) no-repeat;\n       background-size: cover; \n    }\n\n\n}\n\n/* back-to-top */\n\n#back-to-top{\n    cursor: pointer;\n    color:#26262283;\n    position: fixed;\n    bottom:3rem;\n    right:3rem;\n    font-size:2rem;\n    border: 2px solid #26262283;\n    padding:0.5rem 1rem;\nbackground-color: transparent;\noutline:0;\nz-index: 1000;\n}\n\n#back-to-top:hover{\n    background-color:#fff;\n}\n\n@media  screen and (min-width:768px) {\n#back-to-top{    \n    font-size:3rem;\n    padding:0.5rem 1.5rem;\n    border: 3px solid #26262283;\n}\n}\n\n/* block */\n\n.block{\n    --padding-vertical:6rem;\n    padding:var(--padding-vertical) 2rem;\n}\n\n.block__header{\n    text-align: center;\n}\n\n/* margin */\n\n.margin--block{\n    margin:0 2rem;\n  }\n  .margin-bottom--small{\n    margin-bottom:1.5rem;\n}\n\n/* text-align */\n\n.text-align--left{\n  text-align:left;\n}  \n\n@media screen and (min-width:768px) {\n   .text-align-tablet--left{\n    text-align:left;\n}  \n}\n\n/* hero */\n\n.hero__heading{\n    text-transform: uppercase;   \n}\n\n.hero-block{\n    padding:2rem 2rem 6rem;\n}\n\n\n@media screen and (min-width:375px){\n    .hero-block{\n        padding:10rem 2rem 10rem;\n}\n}\n\n@media screen and (min-width:1024px){\n    .hero-block{\n        padding:15rem 2rem;\n}\n}\n\n@media screen and (min-width:1300px){\n    .hero-block{\n        padding:0 2rem 6rem;\n}\n}\n\n\n\n/* button */\n\n.btn{\n    padding:1rem 4rem;\n    outline:0;\n    border:0.1px solid #fff;\n    background-color: transparent;\n    cursor:pointer;\n    white-space:nowrap;\n    text-align: center;\n    margin:1rem 0;\n    color:#fff;\n    text-transform: uppercase;\n    font-size: 1.2rem;\n}\n\n.btn:hover{\n    background-color:#f2eeeb7a ;\n}\n\n@media screen and (min-width:600px){\n    .btn{\n        font-size:1.8rem;\n        letter-spacing: 2px; \n    }\n}\n\n@media screen and (min-width:768px){\n    .btn{\n        font-size:2rem;\n        letter-spacing: 2px; \n    }\n}\n\n@media screen and (min-width:1024px){\n    .btn{\n        font-size:2.5rem;\n    }\n}\n\n/* icon */\n\n.icon{\n    width:40px;\n    height:40px;\n}\n\n.icon--secondary{\n    fill:var(--color-secondary);\n}\n\n.icon--white{\n    fill:#fff;\n}\n\n.icon--pin{\n    margin:0 0.4rem 0 0.4rem;\n}\n\n.icon--fee{\n font-size:3rem;\npadding-bottom:0.5rem;\n}\n\n/* callopsible */\n.collapsible__header{\n    display:flex;\n    justify-content: space-between;\n    align-items: flex-start;\n    border-bottom:0.5px solid #fff;\n    margin-bottom: 1rem;\n}\n\n.collapsible__heading{\n    margin-top:0;\n}\n\n.collapsible__chevron{\n    transform: rotate(-90deg);\n    transition:transform 0.3s;\n}\n\n.collapsible__content{\n    max-height:0;\n    overflow: hidden;\n    opacity:0;\n    transition:all 0.3s;\n}\n\n.collapsible--expanded .collapsible__chevron{\n    transform: rotate(0);\n}\n\n.collapsible--expanded .collapsible__content{\n    max-height:100vh;\n    opacity:1;\n}\n\n/* list */\n\n.list{\n    list-style: none;\n    padding-left:0;\n}\n\n.list__content{\n    margin-top:0;\n    padding-left:0;\n}\n\n/* nav */\n\n.nav__brand{\n    padding-top: 0.5rem;\n    padding-left:1rem;\n}\n.nav__logo{\n    width:4rem;\n    height:4rem;\n}\n\n.nav{\n    display:flex;\n    justify-content: space-between;\n    flex-wrap:wrap;\n\n    align-items: center;\nfont-weight: 400; \n    text-transform: uppercase;\nalign-items:center;\nwidth:100vw;\nbackground-color: #f2eeeb37;\n}\n\n\n\n.nav__list{\n    width:100%;\n    margin:0;\n}\n\n.nav__item{\n    padding:1.5rem 2rem;\n    border-bottom: 0.2px solid var(--color-primary);\n\n}\n\n.nav__item>a{\n    color:#262622;\n}\n\n.nav__item:hover>a{\n    color:var(--color-primary);\n    transition:color 0.3s;\n}\n\n.nav__item:hover{\n    background-color: var(--color-accent);\n}\n\n.nav__toggler{\n    opacity:1;\n    cursor:pointer;\n  position:absolute;\n  top:2px;\n  right:0;\n  margin:0.6rem 0.8rem 0 ;\nfill:var(--color-accent); \n  transition:all 0.15s;\n}\n\n.nav__toggler__close{\n    opacity:0;\n   width: 25px;\n   margin:1rem 1.6rem 0 ;\n   fill:#fff;\n}\n\n.nav.collapsible--expanded .nav__toggler{\n    opacity:0; \n    fill:#fff;\n}\n.nav.collapsible--expanded .nav__toggler__close{\n    opacity:1; \n    fill:var(--color-accent);\n    \n}\n\n@media screen and (min-width:768px){\n\n\n    .nav__toggler, .nav__toggler__close{\n        display:none;\n    }\n\n    \n\n    .nav__list{\n        max-height:100%;\n        opacity: 1;\n        display:flex;\n        width:auto;\n    }\n    \n    .nav__item:hover>a{\n        color:var(--color-primary);\n    }\n\n    .nav__item:hover{\n        background-color: var(--color-accent);\n        transition:background-color 0.3s;\n    }\n\n    .nav-bottom-border{\n        border-bottom: 0.5px solid var(--color-primary);\n     }   \n     \n     .nav__item{\n    border-bottom: none;\n}\n}\n\n\n \n\n/* grid */\n\n.grid{\n    display:grid;\n}\n\n@media screen and (min-width:768px){\n.grid--1x2{\n    grid-template-columns: repeat(2,1fr);\n}\n.grid--1x3{\n    grid-templ