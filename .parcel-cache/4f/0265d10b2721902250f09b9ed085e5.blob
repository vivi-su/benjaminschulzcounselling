    z-index: 2;   
}

/* style-wraper */

.style-container{
    position:relative;   
}

.style{
    border:1px solid var(--color-primary);
    position: absolute;
    top:20%;
    left:20%;
    z-index:1;
}

.style-wraper{
    width:70%;
    height: 0;
    padding-bottom: 70%;
}

@media screen and (min-width:768px){
  
    .style-wraper{
        width:72%;
        height: 0;
        padding-bottom: 72%;
    }
    
}

@media screen and (min-width:1024px){
  
    .style-wraper{
        width:65%;
